  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "worker": "tsx worker/worker.ts",
    "worker:build": "tsc worker/worker.ts --outDir dist",
    "worker:prod": "node dist/worker/worker.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "agents:check": "node -e \"console.log('Checking agent configurations...'); require('./lib/openai-agents.ts').getTrainedAgents().forEach(a => console.log('âœ…', a.name, a.isTrained ? 'trained' : 'legacy'))\"",
    "agents:list": "node -e \"const agents = require('./lib/openai-agents.ts').TRAINED_AGENTS; Object.entries(agents).forEach(([id, config]) => console.log(id + ':', config.name, '-', config.promptId ? 'Trained' : 'Legacy'))\"",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
